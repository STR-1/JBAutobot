			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(cmds2))


	elseif Message:lower() == ">autocrash" and not table.find(_G.Blacklist, Player.UserId) then
		if _G.AutoCrashRunning == false then
			_G.AutoCrashRunning = true
			_G.CanJoinCrash = true
			_G.AddMultiplier = false

			local Crashes = {math.random(0,15), math.random(1,10), math.random(0,20)}
			_G.CrashTime = Crashes[math.random(1, #Crashes)]
			_G.CrashX = 0
			_G.CrashPlayers = {}


			local CrashStart = {
				[1] = "AutoCrash has started! AutoCrash will begin in 20s, respond '>join bet [Level Amount]' for a chance at up to 20X your Levels!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CrashStart))

			task.spawn(function()
				local Count = 0
				repeat
					task.wait(1)
					Count = Count + 1
				until Count == 20

				local CrashStart = {
					[1] = "AutoCrash has started! Respond '>cashout' before AutoCrash crashes! If you do not cashout before AutoCrash crashes, you loose all your betted levels.",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CrashStart))
				_G.CanCashout = true
				_G.CanJoinCrash = false
				_G.AddMultiplier = true

				task.spawn(function()
					repeat
						task.wait(1)
						_G.CrashX = _G.CrashX + 1
					until _G.AddMultiplier == false
				end)

				task.spawn(function()
					repeat
						if _G.CrashX == 5 then
							local Alrt = {
								[1] = "AutoCrash reached 5X",
								[2] = "All"
							}
							game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(Alrt))
							task.wait(5)
						elseif _G.CrashX == 10 then
							local Alrt = {
								[1] = "AutoCrash reached 10X",
								[2] = "All"
							}
							game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(Alrt))
							task.wait(5)
						elseif _G.CrashX == 15 then
							local Alrt = {
								[1] = "AutoCrash reached 15X",
								[2] = "All"
							}
							game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(Alrt))
							task.wait(5)		
						end
						task.wait()
					until _G.AddMultiplier == false
				end)

				task.spawn(function()
					task.wait(_G.CrashTime)
					_G.CanCashout = false
					local CrashStart = {
						[1] = EmojisPath["Bang"].."AutoCrash crashed at ".._G.CrashX.."X"..EmojisPath["Bang"],
						[2] = "All"
					}
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CrashStart))

					local PostCrashLog = {
						["embeds"] = {{
							["title"] = EmojisPath["Bang"].."AutoCrash crashed @ ".._G.CrashX.."X"..EmojisPath["Bang"],
							["description"] = "AutoBot #".._G.AutobotID.." @ "..os.date(),
							["color"] = 15548997,
							["fields"] = {
								{
									["name"] = "System Gain:",
									["value"] = GetFormattedNumber(_G.CrashGain),
								},
								{
									["name"] = "System Loss:",
									["value"] = GetFormattedNumber(_G.CrashLoss),
								}
						}}
					}}
				
					PostCrash(PostCrashLog)

					_G.CrashGain = 0
					_G.CrashLoss = 0
					_G.AutoCrashRunning = false
					_G.AddMultiplier = false
					SaveAutobotData()
					_G.CrashPlayers = {}
				end)
			end)
		else
			local CrashRunning = {
				[1] = "[ERROR!] AutoCrash is already running! To join respond '>join bet [Level Amount]'",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CrashRunning))
		end

	elseif not Message:lower():match("10s") and not Message:lower():match("[ERROR!]") and Message:lower():match(">join bet") and not table.find(_G.Blacklist, Player.UserId) and _G.CanJoinCrash == true then
		if not _G.CrashPlayers[tostring(Player.UserId)] then
			local MessageSplit = string.split(Message, " ")
			local BetAmount = GetFormattedText(MessageSplit[3]) or tonumber(MessageSplit[3])

			if BetAmount then
					if BetAmount > 0 then
						if _G.Data["Levels"][tostring(Player.UserId)]["Level"] >= BetAmount then
							local BetError = {
								[1] = Player.DisplayName.." betted "..GetFormattedNumber(BetAmount).." Levels on AutoCrash!",
								[2] = "All"
							}
							game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))
	
							_G.Data["Levels"][tostring(Player.UserId)]["Level"] = _G.Data["Levels"][tostring(Player.UserId)]["Level"] - BetAmount
							_G.CrashPlayers[tostring(Player.UserId)] = {
								["BetAmount"] = BetAmount,
							}
							
							_G.CrashGain = _G.CrashGain + BetAmount

							SaveAutobotData()

							local PostCrashLog = {
								["embeds"] = {{
									["title"] = Player.DisplayName.." betted "..GetFormattedNumber(BetAmount).." Levels on AutoCrash!",
									["color"] = 15548997,
									["description"] = "AutoBot #".._G.AutobotID.." @ "..os.date(),
									["fields"] = {
										{
											["name"] = "Levels Remaining: ",
											["value"] = _G.Data["Levels"][tostring(Player.UserId)]["Level"],
										},
								}}
							}}
						
							PostCrash(PostCrashLog)
						else
							local BetError = {
								[1] = "[ERROR!] "..Player.DisplayName.." you do not have "..GetFormattedNumber(BetAmount).." Levels to bet.",
								[2] = "All"
							}
							game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))
						end
					else
						local BetError = {
							[1] = "[ERROR!] Bet must be greater than 0!",
							[2] = "All"
						}
						game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))
					end
			else
				local BetError = {
					[1] = "[ERROR!] Bet must be a number value!",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))
			end
		end

	elseif not Message:lower():match("betted") and Message:lower():match(">cashout") and not table.find(_G.Blacklist, Player.UserId) and _G.CanCashout == true then
		local BetReward = _G.CrashPlayers[tostring(Player.UserId)]["BetAmount"] * tonumber(_G.CrashX)

		_G.Data["Levels"][tostring(Player.UserId)]["Level"] = _G.Data["Levels"][tostring(Player.UserId)]["Level"] + BetReward

		local BetError = {
			[1] = EmojisPath["Check"].." "..Player.DisplayName.." cashed out at ".._G.CrashX.."X and got +"..GetFormattedNumber(BetReward).." Levels",
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))

		local PostCrashLog = {
			["embeds"] = {{
				["title"] = EmojisPath["Check"].." "..Player.DisplayName.." cashed out at ".._G.CrashX.."X and got +"..GetFormatted2(BetReward).." Levels",
				["color"] = 15548997,
				["description"] = "AutoBot #".._G.AutobotID.." @ "..os.date(),
				["fields"] = {
					{
						["name"] = "Level: ",
						["value"] = _G.Data["Levels"][tostring(Player.UserId)]["Level"],
					},
			}}
		}}
	
		PostCrash(PostCrashLog)

		_G.CrashPlayers[tostring(Player.UserId)] = nil

		_G.CrashLoss = _G.CrashLoss + BetReward

		SaveAutobotData()

	elseif not table.find(_G.Blacklist, Player.UserId) and Message:lower() == ">colorbet" then
	
		local BetError = {
			[1] = "To play ColorBet first choose a Color (Blue = 2x | Red = 2x | White = 5x) and respond '>ColorBet [Color] [Level Amount]' if your Color is not rolled, you loose all your betted levels.",
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))


	elseif not table.find(_G.Blacklist, Player.UserId) and table.find(Colors, Message:lower():split(" ")[2]) then
		local Split = Message:lower():split(" ")
		local SelectedColor = Colors[math.random(1, #Colors)]
		local BetAmount = GetFormattedText(Split[3]) or tonumber(Split[3])
		if _G.Data["Levels"][tostring(Player.UserId)]["Level"] > BetAmount then
			if BetAmount then
				if BetAmount > 0 then
					local ColorToValue = {
						["blue"] = 2,
						["red"] = 2,
						["white"] = 5
					}
					local ColorToEmoji = {
						["blue"] = EmojisPath["ColorBlue"],
						["red"] = EmojisPath["ColorRed"],
						["white"] = EmojisPath["ColorWhite"]
					}
					
					_G.Data["Levels"][tostring(Player.UserId)]["Level"] = _G.Data["Levels"][tostring(Player.UserId)]["Level"] - BetAmount
					if SelectedColor == Split[2] then
						local BetError = {
							[1] = "ColorBet rolled... ("..ColorToEmoji[SelectedColor]..") | "..Player.DisplayName.." you got +"..GetFormattedNumber(BetAmount * ColorToValue[Split[2]]).." Levels!",
							[2] = "All"
						}
						game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))
						_G.Data["Levels"][tostring(Player.UserId)]["Level"] = _G.Data["Levels"][tostring(Player.UserId)]["Level"] + BetAmount * ColorToValue[Split[2]]
						SaveAutobotData()

						local PostCrashLog = {
							["embeds"] = {{
								["title"] =  Player.DisplayName.." betted "..GetFormatted2(BetAmount).." on ("..ColorToEmoji[Split[2]]..")",
								["color"] = 15548997,
								["description"] = "AutoBot #".._G.AutobotID.." @ "..os.date(),
								["fields"] = {
									{
										["name"] = "ColorBet rolled...",
										["value"] = "("..ColorToEmoji[SelectedColor]..")",
									},
									{
										["name"] = "WIN!",
										["value"] = Player.DisplayName.." got +"..GetFormatted2(BetAmount * ColorToValue[Split[2]]).." Levels!"
									},
							}}
						}}
					
						PostColorBet(PostCrashLog)
					else
						local BetError = {
							[1] = "ColorBet rolled... ("..ColorToEmoji[SelectedColor]..")",
							[2] = "All"
						}
						game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))

						local PostCrashLog = {
							["embeds"] = {{
								["title"] =  Player.DisplayName.." betted "..GetFormatted2(BetAmount).." on ("..ColorToEmoji[Split[2]]..")",
								["color"] = 15548997,
								["description"] = "AutoBot #".._G.AutobotID.." @ "..os.date(),
								["fields"] = {
									{
										["name"] = "ColorBet rolled...",
										["value"] = "("..ColorToEmoji[SelectedColor]..")",
									},
							}}
						}}
					
						PostColorBet(PostCrashLog)
					end
				else
					local BetError = {
						[1] = "[ERROR!] Bet must be greater than 0!",
						[2] = "All"
					}
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))
				end
				else
					local BetError = {
						[1] = "[ERROR!] Bet must be a number value!",
						[2] = "All"
					}
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))
				end
		else
			local BetError = {
				[1] = "[ERROR!] "..Player.DisplayName.." you do not have "..BetAmount.." Levels to ColorBet",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))
		end

	elseif not table.find(_G.Blacklist, Player.UserId) and Message:lower() == ">battle" then
		if _G.BattleRunning == false then
			if _G.Data["Levels"][tostring(Player.UserId)]["Level"] > 5 then
				_G.BattleRunning = true
				_G.BattleWaitForPlayer = true
				_G.BattlePlayers["Player1"] = {
					["UserId"] = Player.UserId 
				}

				_G.Data["Levels"][tostring(Player.UserId)]["Level"] = _G.Data["Levels"][tostring(Player.UserId)]["Level"] - 5
				local BetError = {
					[1] =  Player.DisplayName.." has started a Battle, respond '>join battle' or Battle against Auto with '>join battle bot'",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))

				task.spawn(function()
					repeat task.wait() until _G.BattleWaitForPlayer == false
					
					local Player1 = game:GetService("Players"):GetPlayerByUserId(_G.BattlePlayers["Player1"]["UserId"])
					local Player2 = game:GetService("Players"):GetPlayerByUserId(_G.BattlePlayers["Player2"]["UserId"])

					local Player1Rarity = GetBattleRarity(math.random(2,100))
					local Player2Rarity = GetBattleRarity(math.random(2,100))

					local Player1BattleItem = GetBattleItem(Player1Rarity)
					local Player2BattleItem = GetBattleItem(Player2Rarity)
					local Winner
					local WinnerValue

					if Player1BattleItem.Value > Player2BattleItem.Value then
						Winner = Player1
						WinnerValue = Player1BattleItem.Value
					elseif  Player2BattleItem.Value > Player1BattleItem.Value  then
						Winner = Player2
						WinnerValue = Player2BattleItem.Value
					elseif Player1BattleItem.Value == Player2BattleItem.Value  then
						local RandomWinner = {Player1, Player2}
						local ChosenRandomWinner = RandomWinner[math.random(1, #RandomWinner)]

						if ChosenRandomWinner == Player1 then
							Winner = Player1
							WinnerValue = Player1BattleItem.Value

						elseif ChosenRandomWinner == Player2 then
							Winner = Player2
							WinnerValue = Player2BattleItem.Value
						end
					end

					local BetError = {
						[1] =  "P1 got a "..Player1Rarity:upper().." "..Player1BattleItem.Name:upper().." worth "..tostring(GetFormattedNumber(Player1BattleItem.Value)).." Levels! |".." P2 got a "..Player2Rarity:upper().." "..Player2BattleItem.Name:upper().." worth "..GetFormattedNumber(Player2BattleItem.Value).." Levels! | 🎉 "..Winner.DisplayName.." won "..tostring(GetFormattedNumber(WinnerValue)).." Levels! 🎉",
						[2] = "All"
					}
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))

					local BattleLogData = {
						["embeds"] = {{
							["title"] = "Battle",
							["description"] = "AutoBot #".._G.AutobotID.." @ "..os.date(),
							["color"] = 15548997,
							["fields"] = {
								{
									["name"] = Player1.DisplayName.." VS ".. Player2.DisplayName,
									["value"] = "Fairness: "..math.random(1,100)
								},
								{
									["name"] = Player1.DisplayName..":",
									["value"] = " gota "..Player1Rarity:upper().." "..Player1BattleItem.Name:upper().." worth "..tostring(GetFormattedNumber(Player1BattleItem.Value)).." Levels!",
								},
								{
									["name"] = Player2.DisplayName..":",
									["value"] = "got a "..Player2Rarity:upper().." "..Player2BattleItem.Name:upper().." worth "..GetFormattedNumber(Player2BattleItem.Value).." Levels!"
								},

								{
									["name"] = "Winner:",
									["value"] = "🎉"..Winner.DisplayName.." won "..tostring(GetFormattedNumber(WinnerValue)).." Levels!🎉"
								},
						}}
					}}
				
					 PostBattle(BattleLogData)
					
					_G.Data["Levels"][tostring(Winner.UserId)]["Level"] = _G.Data["Levels"][tostring(Winner.UserId)]["Level"] + WinnerValue
					_G.BattleRunning = false
					SaveAutobotData()
				end)

			else
				local BetError = {
					[1] =  Player.DisplayName.." you do not 5 Levels to start a battle.",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))
			end
		else
			local BetError = {
				[1] = "A Battle is already running, to join respond '>join battle' or Battle against Auto with '>join battle bot' [COST 5 Levels]",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))
		end


	elseif not table.find(_G.Blacklist, Player.UserId) and Message:lower() == ">join battle" then
		if Player.UserId == _G.BattlePlayers["Player1"]["UserId"] then return end 
		if _G.BattleRunning == true then
			if _G.Data["Levels"][tostring(Player.UserId)]["Level"] > 5 then
				_G.Data["Levels"][tostring(Player.UserId)]["Level"] = _G.Data["Levels"][tostring(Player.UserId)]["Level"] - 5
				_G.BattlePlayers["Player2"] = {
					["UserId"] = Player.UserId
				}

				_G.BattleWaitForPlayer = false
			else
				local BetError = {
					[1] =  Player.DisplayName.." you do not enough Levels to start a battle. [5 Levels]",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))
		end
		else
			local BetError = {
				[1] = "A Battle is not running, respond '>battle' to start a Battle.",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))
		end

	elseif not table.find(_G.Blacklist, Player.UserId) and Message:lower() == ">join battle bot" then
		if _G.BattleRunning == true then
			_G.BattlePlayers["Player2"] = {
				["UserId"] = game.Players.LocalPlayer.UserId
			}

			_G.BattleWaitForPlayer = false
		else
			local BetError = {
				[1] = "A Battle is not running, respond '>battle' to start a Battle.",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))
		end
		
	elseif not table.find(_G.Blacklist, Player.UserId) and Message:lower() == ">tomb" then
		local BetError = {
			
			[1] = "AutoBot is standing on the button closest to prison, 2 more players are required...",
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))

		task.spawn(function()
			local Count  = 0
			repeat
			task.wait()
			Count = Count + 1
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(528.431, 25.904, -480.641)
			until Count == 100
		end)

	elseif not table.find(_G.Blacklist, Player.UserId) and Message:lower() == ">buy" then
		local postad = {
			[1] = "Buy Autobot at dsc.gg/JBAUTOBOT |600 R$ |5$ (AUTOBOT PURCHASES ARE BEING DISCONTUINED FEB 1ST 2023, BUY NOW OR NEVER.)",
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))

	elseif not table.find(_G.Blacklist, Player.UserId) and Message:lower():match(">lyft to") then

		if Message:lower():match("level") or Message:lower():match(">pull,") then return end

		local GunShops = {CFrame.new(371.845, 27.888, 537.329), CFrame.new(18.713, 22.388, -1770.933)}
		local Mansions = {CFrame.new(3043.941, 61.774, -4686.663), CFrame.new(3043.941, 61.774, -4527.653)}
		local Escapes = {CFrame.new(-1886.857, 245.407, -1494.66), CFrame.new(-1706.461, 68.525, -2035.186)}
		local HideOuts = {CFrame.new(3334.405, 81.025, -4421.161), CFrame.new(2206.714, 53.313, 912.521), CFrame.new(-568.228, 184.173, 2741.587), CFrame.new(909.775, 146.303, -4131.387), CFrame.new(-550.235, 21.483, -3444.338)}
		local Jstores = {CFrame.new(163.103, 15.766, 1359.797), CFrame.new(135.899, 131.632, 1303.44), CFrame.new(142.94, 116.863, 1327.507)}
		local LyftPlaces = {
			["prison"] = CFrame.new(-1219.906, 28.551, -1758.503),
			["1m"] = CFrame.new(484.984, 16.718, -1583.255),
			["tomb"] = CFrame.new(607.649, 29.224, -479.041),
			["gunshop"] = GunShops[math.random(1, #GunShops)],
			["donut"] = CFrame.new(100.246, 21.323, -1504.176),
			["mansion"] = Mansions[math.random(1, #Mansions)],
			["casino"] = CFrame.new(-197.194, 17.014, -4578.103),
			["bank"] = CFrame.new(-8.668, 21.22, 866.22),
			["jstore"] = Jstores[math.random(1, #Jstores)],
			["powerplant"] = CFrame.new(79.707, 18.766, 2339.862),
			["cargoplane"] = CFrame.new(-1238.74, 39.072, 2854.42),
			["msm"] = CFrame.new(1109.898, 136.959, 1320.003),
			["boat"] = CFrame.new(2681.862, 29.781, -3806.072),
			["volcano"] = CFrame.new(2238.773, 18.465, -1972.316),
			["gas"] = CFrame.new(-1638.836, 27.88, 694.3931),
			["escape"] = Escapes[math.random(1, #Escapes)],
			["hideout"] = HideOuts[math.random(1, #HideOuts)]

		}

		if game.Players.LocalPlayer.Folder:FindFirstChild("Cuffed") then
			local postad = {
				[1] = "[ERROR] AutoBot has been arrested. lyft command disabled for 20 seconds.",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))

			task.spawn(function()
				for x = 1, 20 do
					task.wait(1)
				end
			end)
			return
		end

		if _G.LyftInProgress == true then
			local postad = {
				[1] = "[ERROR] Lyft already in progress!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end

		if _G.Data["Levels"][tostring(Player.UserId)]["Level"] >= 50 == false then
			local postad = {
				[1] = "[ERROR] You need Level 50+ to use this command!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end
		
		local Split = Message:lower():split(" ")

		if not LyftPlaces[Split[3]] then
			local postad = {
				[1] =  "[ERROR] "..Split[3].." is not a valid place to Lyft to. Say >Places for a valid list.",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end

		local Model = Instance.new("Model")
		Model.Parent = game.workspace

		local BasePart = Instance.new("Part")
		BasePart.Parent = Model
		BasePart.Anchored = true
		BasePart.Size = Vector3.new(0.305, 0.193, 0.355)
		BasePart.Position = Vector3.new(-1221.497, 23.455, -1780.061)
		BasePart.CanCollide = false

		local Part = Instance.new("Part")
		Part.Parent = Model
		Part.Anchored = true
		Part.Size = Vector3.new(0.305, 239.878, 0.355)
		Part.Position = Vector3.new(-1220.152, 143.279, -1781.664)
		Part.CanCollide = true
		
		Model.PrimaryPart = BasePart
		Model:SetPrimaryPartCFrame(CFrame.new(Player.Character.HumanoidRootPart.Position))

		for _, Part in pairs(Part:GetTouchingParts()) do
			if Part then
				_G.IsCurrentInBuilding = true
			else
				_G.IsCurrentInBuilding = false
			end
		end

		if _G.IsCurrentInBuilding == true then
			_G.IsCurrentInBuilding = false
			local postad = {
				[1] =  "[ERROR] You cannot request a lyft while in a building!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end

		_G.LyftInProgress = true
		local LyftPlace = LyftPlaces[Split[3]]

		local postad = {
			[1] = "AutoBot is on the way to Lyft you to "..Split[3]..", once he arrives get in the passenger seat as quickly as possible.",
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))

		local thread = task.spawn(function()
			Lyft(CFrame.new(BasePart.Position), true, true)
	
			task.spawn(function()
				local VirtualInputManager = game:GetService"VirtualInputManager"
				VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.H, false, workspace)
	
				task.wait(0.1)
	
				VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.H, false, workspace)
				
				task.wait(0.1)
	
				VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.H, false, workspace)
	
				task.wait(0.1)
	
				VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.H, false, workspace)
	
				task.wait(0.1)
	
				VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.H, false, workspace)
	
				task.wait(1)
	
				VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.H, false, workspace)
			end)
	
			task.wait(5)

			Lyft(LyftPlace, true, true)

			task.wait(5)
	
			Lyft(LyftPlaces["hideout"], false, true)

			_G.LyftInProgress = false
	
			Model:Destroy()
			local postad = {
				[1] = "Lyft has been completed!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
	
			_G.LyftInProgress = false
	end)

	--[[task.delay(, function()
	  task.cancel(thread)
	  _G.LyftInProgress = false
	end)]]

	elseif not table.find(_G.Blacklist, Player.UserId) and Message:lower() == ">places" then
		local postad = {
			[1] = "AutoBot lyft Places: Prison, 1M, Volcano, GunShop, Boat, Escape, Donut, Gas, Bank, JStore, MSM, PowerPlant, CargoPlane, Tomb, Casino, Mansion",
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))

	elseif not table.find(_G.Blacklist, Player.UserId) and Message:lower():match(">drop") then
		if Message:lower():match("level") then return end

		if _G.Data["Levels"][tostring(Player.UserId)]["Level"] >= 200 == false then
			local postad = {
				[1] = "[ERROR] You need Level 200+ to use this command!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end

		if _G.DroppedLevelData["Active"] then
			local postad = {
				[1] = "[ERROR] Someone has already dropped Levels! Respond '>Claim' before dropping more Levels.",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end

		local Split = Message:lower():split(" ")
		local Amount = GetFormattedText(Split[2]) or tonumber(Split[2])

		if not Amount then
			local postad = {
				[1] = "[ERROR] Amount must be a Number value!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end

		if Amount <= 0 then
			local postad = {
				[1] = "[ERROR] Amount must be greater than 0!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end

		if _G.Data["Levels"][tostring(Player.UserId)]["Level"] < Amount then
			local postad = {
				[1] = "[ERROR] You don't have "..Amount.." Levels to Drop!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end

		_G.DroppedLevelData["Active"] = true
		_G.DroppedLevelData["Amount"] = Amount
		_G.DroppedLevelData["Origin"] = Player

		local postad = {
			[1] = EmojisPath["Party2"]..Player.DisplayName.." dropped "..GetFormattedNumber(Amount).." Levels, respond '>Claim' to claim these Levels!"..EmojisPath["Party2"],
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))

	elseif not table.find(_G.Blacklist, Player.UserId) and Message:lower() == ">claim" and _G.DroppedLevelData["Active"] then
		_G.Data["Levels"][tostring(Player.UserId)]["Level"] = _G.Data["Levels"][tostring(Player.UserId)]["Level"] + _G.DroppedLevelData["Amount"]
		_G.Data["Levels"][tostring(_G.DroppedLevelData["Origin"].UserId)]["Level"] = _G.Data["Levels"][tostring(_G.DroppedLevelData["Origin"].UserId)]["Level"] - _G.DroppedLevelData["Amount"]
	
		local postad = {
			[1] = EmojisPath["Party2"]..Player.DisplayName.." claimed "..GetFormattedNumber(_G.DroppedLevelData["Amount"]).. " Levels from ".._G.DroppedLevelData["Origin"].DisplayName.."!"..EmojisPath["Party2"],
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))

		_G.DroppedLevelData = {}
		SaveAutobotData()

	elseif not table.find(_G.Blacklist, Player.UserId) and Message:lower():match(">give") then
		if Message:lower():match("level") then return end

		if _G.Data["Levels"][tostring(Player.UserId)]["Level"] >= 200 == false then
			local postad = {
				[1] = "[ERROR] You need Level 200+ to use this command!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end

		local Splitted = string.split(Message, " ")
		local LevelValue = GetFormattedText(Split[3]) or tonumber(Split[3])
		local PersonToAddLevel = Splitted[2]
		local PersonToAddLevelDisplayName

		for _, Players in pairs(game.Players:GetChildren()) do
			if string.sub(Players.DisplayName:lower(), 1, string.len(PersonToAddLevel)) == PersonToAddLevel:lower() then
				PersonToAddLevelDisplayName = Players
			end
		end

		if PersonToAddLevelDisplayName.UserId == Player.UserId then
			local postad = {
				[1] = "[ERROR] You cannot run this command on yourself!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end

		if not tonumber(LevelValue) then
			local postad = {
				[1] = "[ERROR] Amount must be a Number value!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end

		if tonumber(LevelValue) <= 0 then
			local postad = {
				[1] = "[ERROR] Amount must be greater than 0!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end

		if _G.Data["Levels"][tostring(Player.UserId)]["Level"] < tonumber(LevelValue) then
			local postad = {
				[1] = "[ERROR] You don't have "..GetFormattedNumber(LevelValue).." Levels to Give!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end

		_G.Data["Levels"][tostring(PersonToAddLevelDisplayName.UserId)]["Level"] = _G.Data["Levels"][tostring(PersonToAddLevelDisplayName.UserId)]["Level"] + tonumber(LevelValue)
		_G.Data["Levels"][tostring(Player.UserId)]["Level"] = _G.Data["Levels"][tostring(Player.UserId)]["Level"] - tonumber(LevelValue)

		local addedlevelargs = {
			[1] = "Added +"..GetFormattedNumber(LevelValue).." Levels to "..PersonToAddLevelDisplayName.DisplayName,
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(addedlevelargs))

		SaveAutobotData()

	elseif not table.find(_G.Blacklist, Player.UserId) and Message:lower() == ">crazytime" and not _G.CrazyTimeInProgress and not Message:lower():match(">lyft to") then
		local Selections = {"0X", "CoinFlip", "EmojiHunt", "Cash or Crash","TopSlot","0X", "CoinFlip", "EmojiHunt", "Cash or Crash","TopSlot", "CRAZY TIME"}
		local RandomSelection = Selections[math.random(1, #Selections)]

		_G.CrazyTimeInProgress = true
		_G.CrazyTimeGain = 0
		_G.CrazyTimeLoss = 0
		_G.CrazyTimeData = {
			["Running"] = true,
			["Selected"] = RandomSelection,
			["GameData"] = {},
			["Players"] = {}
 		}

		local CTmsg = {
			[1] = "CrazyTime has started! CrazyTime will begin in 30s, to join '>join [Amount]'!",
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg)) 

		_G.CanJoinCrazyTime = true

		task.spawn(function()
			task.wait(30)

			local CTmsg = {
				[1] = EmojisPath["Party2"].." The Wheel landed on "..RandomSelection.."! "..EmojisPath["Party2"],
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg))

			local CrazyTimeFunctions = {

				["0X"] = function()
					local CTmsg1 = {
						[1] = "All Players loose their bets this round!",
						[2] = "All"
					}
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg1))


					local PostCTLOG = {
						["embeds"] = {{
							["title"] = "CrazyTime Landed on 0X @ "..os.date(),
							["color"] = 15548997,
							["description"] = "AutoBot #".._G.AutobotID.." @ "..os.date(),
							["fields"] = {
								{
									["name"] = "Gained: "..GetFormatted2(_G.CrazyTimeGain).." Levels",
									["value"] = ""
								},

								{
									["name"] = "Loss: "..GetFormatted2(_G.CrazyTimeLoss).." Levels",
									["value"] = ""
								},

						}}
					}}
				
					PostCT(PostCTLOG)
					_G.CrazyTimeData = {}
					_G.CrazyTimeInProgress = false
					_G.CrazyTimeGain = 0
					_G.CrazyTimeLoss = 0
				end,

				["CoinFlip"] = function()
					local RandomNumber = {1, 2}
					local Red = RandomNumber[math.random(1, #RandomNumber)]
					table.remove(RandomNumber, Red)
					local Blue = RandomNumber[math.random(1, #RandomNumber)]

					local RedValue
					local BlueValue

					if Red == 1 then
						RedValue = math.random(1, 100)
						BlueValue = math.random(1, 20)
					elseif Blue == 1 then
						BlueValue = math.random(1, 100)
						RedValue = math.random(1, 20)
					end

					local CTmsg1 = {
						[1] = EmojisPath["ColorRed"]..EmojisPath["ColorBlue"].." CoinFlip will flip in 5 seconds! Red: "..RedValue.."X".." Blue: "..BlueValue.."X "..EmojisPath["ColorRed"]..EmojisPath["ColorBlue"],
						[2] = "All"
					}
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg1))

					task.wait(5)

					local Colors = {"Red", "Blue"}
					local ColorsToValue = {
						["Blue"] = BlueValue,
						["Red"] = RedValue
					}
					local RandomColor = Colors[math.random(1, #Colors)]

					local CTmsg1 = {
						[1] = EmojisPath["ColorRed"]..EmojisPath["ColorBlue"].." CoinFlip landed on: "..RandomColor.." "..ColorsToValue[RandomColor].."X "..EmojisPath["ColorRed"]..EmojisPath["ColorBlue"],
						[2] = "All"
					}
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg1))

					for Player, Data in pairs(_G.CrazyTimeData["Players"]) do
						_G.Data["Levels"][tostring(Player)]["Level"] = _G.Data["Levels"][tostring(Player)]["Level"] + Data["BetAmount"] * ColorsToValue[RandomColor]

						_G.CrazyTimeLoss = _G.CrazyTimeLoss + Data["BetAmount"] * ColorsToValue[RandomColor]
					end

					SaveAutobotData()

					local PostCTLOG = {
						["embeds"] = {{
							["title"] = EmojisPath["ColorRed"]..EmojisPath["ColorBlue"].." CrazyTime CoinFlip ended @ "..os.date().." "..EmojisPath["ColorRed"]..EmojisPath["ColorBlue"],
							["color"] = 15548997,
							["description"] = "AutoBot #".._G.AutobotID.." @ "..os.date(),
							["fields"] = {
								{
									["name"] = "Blue: "..ColorsToValue["Blue"].."X",
									["value"] = ""
								},

								{
									["name"] = "Red: "..ColorsToValue["Red"].."X",
									["value"] = ""
								},

								{
									["name"] = EmojisPath["ColorRed"]..EmojisPath["ColorBlue"].." The Coin landed on: "..RandomColor.." "..ColorsToValue[RandomColor].."X! "..EmojisPath["ColorRed"]..EmojisPath["ColorBlue"],
									["value"] = ""
								},

								{
									["name"] = "Gained: "..GetFormatted2(_G.CrazyTimeGain).." Levels",
									["value"] = ""
								},

								{
									["name"] = "Loss: "..GetFormatted2(_G.CrazyTimeLoss).." Levels",
									["value"] = ""
								},

						}}
					}}
				
					PostCT(PostCTLOG)

					_G.CrazyTimeData = {}
					_G.CrazyTimeInProgress = false
					_G.CrazyTimeGain = 0
					_G.CrazyTimeLoss = 0
				end,

				["EmojiHunt"] = function()
					local Emojis = {EmojisPath["Clover"],EmojisPath["Star"], EmojisPath["SpaceShip"]}

					_G.CanJoinHunt = true

					local CTmsg1 = {
						[1] = "EmojiHunt will begin in 15s, choose an Emoji (1 = "..EmojisPath["Clover"]..", 2 = "..EmojisPath["Star"]..", 3 = "..EmojisPath["SpaceShip"]..") and say '>select [Number]'!",
						[2] = "All"
					}
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg1))

					local TrueRandomAmount = {1}

					local Multipliers = {
						[1] = 1,
						[2] = 1,
						[3] = 1
					}

					local SelectRandom = math.random(1,3)

					Multipliers[SelectRandom] = math.random(1, 100)

					task.wait(15)
					_G.CanJoinHunt = false

					local ToSort = {
						[1] = {
							["Emoji"] = EmojisPath["Clover"],
							["Amount"] = Multipliers[1]
						},

						[2] = {
							["Emoji"] = EmojisPath["Star"],
							["Amount"] = Multipliers[2]
						},

						[3] = {
							["Emoji"] = EmojisPath["SpaceShip"],
							["Amount"] = Multipliers[3]
						},
					}

					table.sort(ToSort, function(Current, Previous)
						return Current["Amount"] > Previous["Amount"]
					end)

					local CTmsg1 = {
						[1] = "EmojiHunt is complete! Winning Emoji: "..ToSort[1]["Emoji"].." = "..ToSort[1]["Amount"].."X!",
						[2] = "All"
					}
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg1))

					for Player, Data in pairs(_G.CrazyTimeData["Players"]) do
						local Win
						if Data["EmojiHuntSelection"] == nil then Win = Data["BetAmount"] * Multipliers[1] end

						if Data["EmojiHuntSelection"] ~= nil then Win = Data["BetAmount"] * Multipliers[Data["EmojiHuntSelection"]] end

						_G.Data["Levels"][tostring(Player)]["Level"] = _G.Data["Levels"][tostring(Player)]["Level"] + Win

						_G.CrazyTimeLoss = _G.CrazyTimeLoss + Win
					end

					SaveAutobotData()

					local PostCTLOG = {
						["embeds"] = {{
							["title"] = EmojisPath["Clover"]..EmojisPath["Clover"].." CrazyTime EmojiHunt ended @ "..os.date()..EmojisPath["Clover"]..EmojisPath["Clover"],
							["color"] = 15548997,
							["description"] = "AutoBot #".._G.AutobotID.." @ "..os.date(),
							["fields"] = {
								{
									["name"] = "Winning Emoji: "..ToSort[1]["Emoji"].." "..ToSort[1]["Amount"].."X!",
									["value"] = ""
								},

								{
									["name"] = "Gained: "..GetFormatted2(_G.CrazyTimeGain).." Levels",
									["value"] = ""
								},

								{
									["name"] = "Loss: "..GetFormatted2(_G.CrazyTimeLoss).." Levels",
									["value"] = ""
								},

						}}
					}}
				
					PostCT(PostCTLOG)

					_G.CrazyTimeData = {}
					_G.CrazyTimeInProgress = false
					_G.CrazyTimeGain = 0
					_G.CrazyTimeLoss = 0
				end,

				["Cash or Crash"] = function()
					_G.CanJoinCashCrash = true
					_G.CashCrashRunning = true

					local CTmsg1 = {
						[1] = "Cash or Crash will begin in 15s, a Ball will be chosen every 10s and the game will continue until a Red Ball is drawn. You can cash out each time a Green Ball is drawn! '>Cashout'",
						[2] = "All"
					}
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg1))
					
					local PostCTLOG = {
						["embeds"] = {{
							["title"] = "CrazyTime Cash or Crash started @ "..os.date(),
							["color"] = 15548997,
							["description"] = "AutoBot #".._G.AutobotID.." @ "..os.date(),
						}
					}}
				
					PostCT(PostCTLOG)

					task.wait(15)

					_G.CanJoinCashCrash = false

					local RandomBall = {"Green","Green","Green","Green","Green","Red","Red","Green","Green","Green","Green","Green","Red","Red","Green","Green","Green","Green","Green","Red","Red", "Green","Green","Green","Green","Green","Red","Red","Green","Green","Green","Green","Green","Red","Red","Green","Green","Green","Green","Green","Red","Red"}

					local Mulitplier = 0
					_G.CashCrashX = 0
					local CurrentSelectedBall

					_G.CanCashCrashCashout = true

					task.spawn(function()
						repeat
							local ChosenBall = RandomBall[math.random(1, #RandomBall)]
							CurrentSelectedBall = ChosenBall

							if ChosenBall == "Green" then

								Mulitplier = Mulitplier + 3

								_G.CashCrashX = Mulitplier

								local CTmsg1 = {
									[1] = " A Green Ball was drawn! Mulitplier is now: "..Mulitplier.."X ",
									[2] = "All"
								}
								game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg1))

								local RemoveBall = table.find(RandomBall, ChosenBall)
								table.remove(RandomBall, RemoveBall)

								local GreenBalls = 0
								local RedBalls = 0
								for _, Ball in pairs(RandomBall) do
									if Ball == "Green" then
										GreenBalls = GreenBalls + 1
									elseif Ball == "Red" then
										RedBalls = RedBalls + 1
									end
								end

								local PostCTLOG = {
									["embeds"] = {{
										["title"] = "Crazy Time Cash or Crash @ "..os.date(),
										["color"] = 15548997,
										["description"] = "AutoBot #".._G.AutobotID.." @ "..os.date(),
										["fields"] = {
											{
												["name"] = "Green Ball was chosen! Mulitplier is now: "..Mulitplier.."X!",
												["value"] = ""
											},
			
											{
												["name"] = "Green Balls left: "..GreenBalls,
												["value"] = ""
											},
			
											{
												["name"] = "Red Balls left: "..RedBalls,
												["value"] = ""
											},
			
									}}
								}}
							
								PostCT(PostCTLOG)
							end
							task.wait(10)
						until ChosenBall == "Red"
						_G.CanCashCrashCashout = false
						local CTmsg1 = {
							[1] = "A Red Ball was drawn! All Players left loose their bets",
							[2] = "All"
						}
						game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg1))

						local PostCTLOG = {
							["embeds"] = {{
								["title"] = "Crazy Time Cash or Crash @ "..os.date(),
								["color"] = 15548997,
								["description"] = "AutoBot #".._G.AutobotID.." @ "..os.date(),
								["fields"] = {
									{
										["name"] = "Red Ball was chosen! All Players left loose their bets.",
										["value"] = ""
									},
							}}
						}}
					
						PostCT(PostCTLOG)

						_G.CashCrashRunning = false
						_G.CrazyTimeData = {}
						_G.CrazyTimeInProgress = false
						_G.CrazyTimeGain = 0
						_G.CrazyTimeLoss = 0
						_G.ForceCrazyTimeMode = false
					end)
				end,

				["CRAZY TIME"] = function()
					local TrueRandomAmount = {
						math.random(50, 100),
						200,
						222,
						300,
						333,
						400,
						444,
						500,
						555,
						600,
						700,
						777,
						800,
						999,
						1000
					}

					local ChosenAmount = TrueRandomAmount[math.random(1, #TrueRandomAmount)]

					local CTmsg1 = {
						[1] = "The CRAZY TIME Wheel will land in 5s...",
						[2] = "All"
					}
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg1))

					task.wait(5)

					local CTmsg1 = {
						[1] = "CRAZY TIME landed on "..ChosenAmount.."X",
						[2] = "All"
					}
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg1))

					for Player, Data in pairs(_G.CrazyTimeData["Players"]) do
						local Win = Data["BetAmount"] * ChosenAmount
						_G.Data["Levels"][tostring(Player)]["Level"] = _G.Data["Levels"][tostring(Player)]["Level"] + Win

						_G.Data["Levels"][tostring(Player)]["LastWin"] = Win
					end

					local PostCTLOG = {
						["embeds"] = {{
							["title"] = "Crazy Time landed on "..ChosenAmount.."X",
							["color"] = 15548997,
							["description"] = "AutoBot #".._G.AutobotID.." @ "..os.date(),

						}}
					}
				
					PostCT(PostCTLOG)

					SaveAutobotData()

					_G.CrazyTimeData = {}
					_G.CrazyTimeInProgress = false
					_G.CrazyTimeGain = 0
					_G.CrazyTimeLoss = 0
					_G.ForceCrazyTimeMode = false
				end,

				["TopSlot"] = function()
					local Multis = {2, 5, 10, 20, 30, 50}
					local Chose = Multis[math.random(1, #Multis)]
					local CTmsg1 = {
						[1] = "CrazyTime TopSlot landed "..Chose.."X",
						[2] = "All"
					}
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg1))

					for Player, Data in pairs(_G.CrazyTimeData["Players"]) do
						local Win = Data["BetAmount"] * Chose
						_G.Data["Levels"][tostring(Player)]["Level"] = _G.Data["Levels"][tostring(Player)]["Level"] + Win

						_G.Data["Levels"][tostring(Player)]["LastWin"] = Win
					end

					local PostCTLOG = {
						["embeds"] = {{
							["title"] = "Crazy Time landed on TopSlot "..Chose.."X",
							["color"] = 15548997,
							["description"] = "AutoBot #".._G.AutobotID.." @ "..os.date(),

						}}
					}
				
					PostCT(PostCTLOG)

					SaveAutobotData()

					_G.CrazyTimeData = {}
					_G.CrazyTimeInProgress = false
					_G.CrazyTimeGain = 0
					_G.CrazyTimeLoss = 0
					_G.ForceCrazyTimeMode = false
				end
			}

			_G.CanJoinCrazyTime = false
			CrazyTimeFunctions[RandomSelection]()

		end)


	elseif not table.find(_G.Blacklist, Player.UserId) and Message:lower():match(">select") and _G.CanJoinHunt and _G.CrazyTimeData["Selected"] == "EmojiHunt" and _G.CrazyTimeData["Players"][Player.UserId] ~= nil then
		local NumberToEmoji = {
			["1"] = EmojisPath["Clover"],
			["2"] =  EmojisPath["Star"],
			["3"] = EmojisPath["SpaceShip"]

		}

		local split = Message:split(" ")

		if not NumberToEmoji[split[2]] then return end

		_G.CrazyTimeData["Players"][Player.UserId]["EmojiHuntSelection"] = tonumber(split[2])

		local CTmsg1 = {
			[1] = Player.DisplayName.. " you selected: "..NumberToEmoji[split[2]],
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg1))

	elseif not table.find(_G.Blacklist, Player.UserId) and Message:lower():match(">join") and _G.CanJoinCrazyTime and not Message:match("CrazyTime has started!") and not _G.CrazyTimeData["Players"][Player.UserId] then
		local Split = Message:split(" ")
		local Amount = GetFormattedText(Split[2]) or tonumber(Split[2])

		if not tonumber(Amount) then
			local postad = {
				[1] = "[ERROR] Bet must be a Number value!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end

		if tonumber(Amount) <= 0 then
			local postad = {
				[1] = "[ERROR] Bet must be greater than 0!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(postad))
			return
		end

		if _G.Data["Levels"][tostring(Player.UserId)]["Level"] < Amount then
			local CTmsg1 = {
				[1] = "[ERROR] You don't have "..GetFormattedNumber(Amount).." Levels to bet on CT!",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg1))
			return
		end

		_G.CrazyTimeData["Players"][Player.UserId] = {
			["BetAmount"] = Amount
		}

		_G.Data["Levels"][tostring(Player.UserId)]["Level"] = _G.Data["Levels"][tostring(Player.UserId)]["Level"] - Amount

		_G.CrazyTimeGain = _G.CrazyTimeGain + Amount
		
		local CTmsg1 = {
			[1] = Player.DisplayName.." betted "..GetFormattedNumber(Amount).." Levels on CT!",
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(CTmsg1))

		SaveAutobotData()
	end

	if not table.find(_G.Blacklist, Player.UserId) and _G.GiveawayActive == true and not table.find(_G.PlayersInGiveaway, Player.DisplayName) and Message:lower() == ">enter" then
		table.insert(_G.PlayersInGiveaway, Player.DisplayName)

		local entered = {
			[1] = EmojisPath["Party2"].." "..Player.DisplayName.." you have been entered into the giveaway for ".._G.GiveawayItem.."! "..EmojisPath["Party2"],
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(entered))
	end

	if _G.CanCopy == true and Player.Name == game.Players.LocalPlayer.Name == false then
		local copyresponseargs = {
			[1] = "["..Player.DisplayName.."]: "..Message,
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(copyresponseargs))
	end

	if _G.CanRun == true and Player.Name == game.Players.LocalPlayer.Name == false then
		wait(math.random(1,5))
		local MessageSize = {
			[1] = Player.DisplayName.." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)]..".",
			[2] = Player.DisplayName.." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)]..".",
			[3] = Player.DisplayName.." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)]..".",
			[4] = Player.DisplayName.." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)]..".",
			[5] = Player.DisplayName.." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)]..".",
			[6] = Player.DisplayName.." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)]..".",
			[7] = Player.DisplayName.." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)]..".",
			[8] = Player.DisplayName.." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)].." ".._G.Messages[math.random(1, #_G.Messages)]..".",
		}

		local messageargs = {
			[1] = MessageSize[math.random(1, #MessageSize)],
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(messageargs[1], messageargs[2])

		local split = string.split(Message, " ")
		for _, each in pairs(split) do 
			for _, brain in pairs(_G.Messages) do
				if not table.find(_G.Messages, each) and not each:match("#") and not each:match(">") then
					table.insert(_G.Messages, each)
				end
			end
		end
	end

	if not table.find(_G.Blacklist, Player.UserId) and _G.HangManRunning == true and table.find(_G.HangManLetters, Message:lower()) then
		for _, Letter in pairs(_G.HangManLetters) do
			if Letter == Message:lower() then
				local Index = table.find(_G.HangManLetters, Message:lower())
				_G.HangManLetters[Index] = "-"
				_G.HangManUnsolvedLetters[Index] = Message:lower()
			end
		end

		if not table.find(_G.HangManUnsolvedLetters, "_") then
			local solvedword = {
				[1] = EmojisPath["Party1"].." "..EmojisPath["Party2"].." ".."The last letter was found! The word was: ("..table.concat(_G.HangManUnsolvedLetters, "")..") "..EmojisPath["Party1"].." "..EmojisPath["Party2"],
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(solvedword[1], solvedword[2])

			_G.HangManUnsolvedLetters = {}
			_G.HangManLetters = {}
			_G.HangManRunning = false
		else
			local foundletter = {
				[1] = EmojisPath["Party1"].." "..EmojisPath["Party2"].." A letter was found! The new word is: ("..table.concat(_G.HangManUnsolvedLetters, "")..") "..EmojisPath["Party1"].." "..EmojisPath["Party2"],
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(foundletter[1], foundletter[2])
		end
	end

	if not table.find(_G.Blacklist, Player.UserId) and _G.HangManRunning == true and Message:lower() == _G.HangManWord then
		local solvedword = {
			[1] = EmojisPath["Party1"].." "..EmojisPath["Party2"].." The word was found! The word was: (".._G.HangManWord..") "..EmojisPath["Party1"].." "..EmojisPath["Party2"],
			[2] = "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(solvedword[1], solvedword[2])

		_G.HangManUnsolvedLetters = {}
		_G.HangManLetters = {}
		_G.HangManRunning = false
	end
end

local Players = game:GetService("Players")
Filtered.OnClientEvent:Connect(function(MSG)
	local Player = Players:FindFirstChild(MSG.FromSpeaker)
	local Message = MSG.Message
	RunAutoBot(Player, Message)
end)

