						[1] =  "P1 got a "..Player1Rarity:upper().." "..Player1BattleItem.Name:upper().." worth "..tostring(GetFormattedNumber(Player1BattleItem.Value)).." Levels! |".." P2 got a "..Player2Rarity:upper().." "..Player2BattleItem.Name:upper().." worth "..GetFormattedNumber(Player2BattleItem.Value).." Levels! | üéâ "..Winner.DisplayName.." won "..tostring(GetFormattedNumber(WinnerValue)).." Levels! üéâ",
						[2] = "All"
					}
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(BetError))
					
					_G.Data["Levels"][tostring(Winner.UserId)]["Level"] = _G.Data["Levels"][tostring(Winner.UserId)]["Level"] + WinnerValue
					_G.BattleRunning = false
