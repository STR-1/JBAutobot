	if not LevelData["Blacklisted"] then
		LevelData["Blacklisted"] = false
	end
end

function GetFormatted2(Number)
	local NumberToString = tostring(Number)

	return NumberToString:reverse():gsub("%d%d%d", "%1,"):reverse():gsub("^,", "")
end

