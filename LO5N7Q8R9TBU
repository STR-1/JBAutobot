					repeat
						task.wait(1)
						_G.CrashX = _G.CrashX + 1
					until _G.AddMultiplier == false
				end)

				task.spawn(function()
					task.wait(_G.CrashTime)
					_G.CanCashout = false
					local CrashStart = {
