				_G.BattleRunning = true
				_G.BattleWaitForPlayer = true
				_G.BattlePlayers["Player1"] = {
					["UserId"] = Player.UserId 
				}

				_G.Data["Levels"][tostring(Player.UserId)]["Level"] = _G.Data["Levels"][tostring(Player.UserId)]["Level"] - 5
